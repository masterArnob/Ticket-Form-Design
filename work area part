<script>
    function showLoader() {
        toastr.info('Fetching data...', 'Loading', {
            closeButton: false,
            timeOut: 0,
            extendedTimeOut: 0,
            tapToDismiss: false
        });
    }

    function hideLoader() {
        toastr.clear();
    }

    async function loadData(btnClick = true) {
        showLoader();
        await Promise.all([
            fetchAgentHrManagementStatistics(btnClick),
            fetchWorkAreaReport(btnClick),
            fetchJobStatusReport(btnClick),
            fetchOfficeTypeWiseReport(btnClick),
            fetchGenderReport(btnClick),
            renderAgentHrReport(),
            fetchSelectedByReport(),
        ]);
        hideLoader();
    }

    loadData(false)

    async function statisticsLocationChange(btnClick) {
        showLoader();
        await Promise.all([
            fetchAgentHrManagementStatistics(btnClick),
            fetchWorkAreaReport(btnClick),
            fetchJobStatusReport(btnClick),
            fetchGenderReport(btnClick),
            renderAgentHrReport(),
            fetchSelectedByReport(btnClick)
        ]);
        hideLoader();
    }
</script>
@endsection
