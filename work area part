<script>
    document.addEventListener('DOMContentLoaded', function() {
        const fullscreenBtn = document.getElementById('fullscreenBtn');
        const dashboardSection = document.getElementById('othoba-dashboard');

        fullscreenBtn.addEventListener('click', function() {
            if (!document.fullscreenElement) {
                // Request fullscreen for the specific section
                dashboardSection.requestFullscreen().then(() => {
                    dashboardSection.style.overflowY = 'auto'; // Enable scrolling
                    this.innerHTML = '<i class="fas fa-compress"></i>';
                }).catch(err => {
                    console.error(`Error trying to enter fullscreen: ${err.message}`);
                });
            } else {
                // Exit fullscreen
                if (document.exitFullscreen) {
                    document.exitFullscreen().then(() => {
                        this.innerHTML = '<i class="fas fa-expand"></i>';
                    }).catch(err => {
                        console.error(`Error trying to exit fullscreen: ${err.message}`);
                    });
                }
            }
        });

    });
</script>

